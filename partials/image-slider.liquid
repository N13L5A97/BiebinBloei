<section class="image-slider-section">
  <h1>{{ headerData.title }}</h1>
  <h2>{{ headerData.subtitle }}</h2>
  <div class="image-slider" id="imageSlider">
    {% for slide in headerData.sliderImages %}
      <div class="slide" style="--background-image: url('{{ slide.src }}')"></div>
    {% endfor %}
  </div>
</section>