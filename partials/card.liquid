<section class="card">
  <article>
    <h2>
      {{ card.title }}
    </h2>
    <div class="p">
      {{ card.content }}
    </div>
    {% include "partials/button.liquid"
      , button: card.button %}
  </article>
  <div class="card-image-container">
    {% if card.image %}
      <img src=" {{ card.image.src }}" alt=" {{ card.image.alt }} ">
    {% endif %}
  </div>
</section>